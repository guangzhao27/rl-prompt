    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal", 
            "cwd": "${fileDirname}"
        },
        {
            "name": "run_EA_fsc",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/EA/run_EA_fsc.py",
            "console": "integratedTerminal",
            "args": [
                "dataset=sst-2",
                "dataset_name=sst-2",
                "dataset_seed=1",
                "prompt_length=5",
                "task_lm=roberta-large",
                "dpo_loss_config.name=ipo",
                "dpo_loss_config.reference_learning_rate=0.001",
                "report_to_wandb=true",
                "dpo_training=false",
                "training_device=cpu",
                "max_train_steps=6000",
                "run_name=roberta-large-rl",
                "report_to_wandb=false", 
                "algorithm_name=ParetoPrompt-IPO",
                "eval_batch_size=2", 
                "dominate_evaluate_num=2",
            ],
            "cwd": "${workspaceFolder}/examples/few-shot-classification/"
        },
        {
            "name": "run_EA",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/EA/run_EA_sts.py",
            "console": "integratedTerminal",
            "args": [
                "dataset=yelp",
                // "dataset_seed=0",
                "top_k=200",
                "direction=0_to_1",
                "prompt_length=2",
                "task_lm=distilgpt2",
                "report_to_wandb=false",
                "dpo_training=true",
                "eval_steps=5",
                "max_train_steps=60",
                "dpo_loss_config.reference_free=true",
                "train_batch_size=2",
                "run_name=newrun",
                "model_path=null", //null or  outputs/2024-07-16/16-04-55
                "training_device='cpu'",
                "load_step=8000",
                "random_seed=42",
                "logit_bias=-10",
                "algorithm_name='ParetoPrompt-IPO'", //
            ],
            "cwd": "${workspaceFolder}/examples/EA/"
        },
        {
            "name": "run_tst",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/text-style-transfer/run_tst.py",
            "console": "integratedTerminal",
            "args": [
                "dataset=yelp",
                // "dataset_seed=0",
                "top_k=200",
                "direction=0_to_1",
                "prompt_length=2",
                "task_lm=distilgpt2",
                "report_to_wandb=false",
                "dpo_training=true",
                "eval_steps=5",
                "max_train_steps=60",
                "dpo_loss_config.reference_free=true",
                "train_batch_size=2",
                "run_name=newrun",
                "model_path=null", //null or  outputs/2024-07-16/16-04-55
                "training_device='cpu'",
                "load_step=8000",
                "random_seed=42",
                "logit_bias=-10",
                "algorithm_name='HVI'",
                "eval_batch_size=2", 
                "dominate_evaluate_num=2",
                "three_objectives=true"
            ],
            "cwd": "${workspaceFolder}/examples/text-style-transfer"
        },
        {
            "name": "run fsc",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/few-shot-classification/run_fsc.py",
            "console": "integratedTerminal",
            "args": [
                "dataset=sst-2",
                "dataset_seed=1",
                "prompt_length=5",
                "task_lm=roberta-large",
                "dpo_loss_config.name=ipo",
                "dpo_loss_config.reference_learning_rate=0.001",
                "report_to_wandb=true",
                "dpo_training=false",
                "training_device=cpu",
                "max_train_steps=6000",
                "run_name=roberta-large-rl",
                "report_to_wandb=false", 
                "algorithm_name=HVI",
                "eval_batch_size=2", 
                "dominate_evaluate_num=2",
                "use_perplexity=true"
            ],
            "cwd": "${workspaceFolder}/examples/few-shot-classification"
        },
        {
            "name": "load model",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/text-style-transfer/load_model/run_tst_load_model.py",
            "console": "integratedTerminal",
            "args": [
                "dataset=yelp",
                "prompt_length=2",
                "task_lm=gpt2-xl",
                "direction=0_to_1",
                "dpo_loss_config.name=ipo",
                "dpo_loss_config.reference_learning_rate=0.001",
                "report_to_wandb=false",
                "dpo_loss_config.dpo_training=true",
                "dpo_loss_config.multi_optimize=true",
                "training_device='cpu'",
                "max_train_steps=12000",
                "project_name=tst-task",
                "model_path=outputs/2024-03-27/15-29-39",
                "run_name=outputs/2024-03-27/15-29-39tst-rl-gpt2-xllalal",
                "prompt_train_batch_size=5",
                "epoch_list=[2000, 4000]",
            ],
            "cwd": "${workspaceFolder}/examples/text-style-transfer/load_model"
        }
    ]
}
